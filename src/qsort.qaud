swap:
	ASS	i, -, temp
	ASS	j, -, i
	ASS	temp, -, j
	FIN	-, -, -
qsort:
	ASS	l, -, i
	ASS	h, -, j
	ADD	i, j, &1
	DIV	&1, 2, &2
	LOAD	a, &2, &3
	ASS	&3, -, m
..@l1:
	LOAD	a, i, &4
	LST	&4, m, ..@l3
	JMP	-, -, ..@l4
..@l3:
..@l5:
	ADD	i, 1, &5
	ASS	&5, -, i
	LOAD	a, i, &6
	GEQ	&6, m, ..@l6
	JMP	-, -, ..@l5
..@l6:
..@l4:
	LOAD	a, j, &7
	LST	m, &7, ..@l7
	JMP	-, -, ..@l8
..@l7:
..@l9:
	SUB	j, 1, &8
	ASS	&8, -, j
	LOAD	a, j, &9
	GEQ	m, &9, ..@l10
	JMP	-, -, ..@l9
..@l10:
..@l8:
	LEQ	i, j, ..@l11
	JMP	-, -, ..@l12
..@l11:
	PUSHA	-, i, *a
	PUSHA	-, j, *a
	CALL	swap, -, -
	ADD	i, 1, &10
	ASS	&10, -, i
	SUB	j, 1, &11
	ASS	&11, -, j
..@l12:
	GTT	i, j, ..@l2
	JMP	-, -, ..@l1
..@l2:
	GTT	j, l, ..@l13
	JMP	-, -, ..@l14
..@l13:
	PUSH	-, -, l
	PUSH	-, -, j
	CALL	qsort, -, -
..@l14:
	LST	i, h, ..@l15
	JMP	-, -, ..@l16
..@l15:
	PUSH	-, -, i
	PUSH	-, -, h
	CALL	qsort, -, -
..@l16:
	FIN	-, -, -
main:
	WRS	-, -, please input number(<10): 
	READ	-, -, num
	ASS	1, -, index
..@l17:
	GTT	index, num, ..@l18
	WRS	-, -, please input number> 
	READ	-, -, temp
	SUB	index, 1, &12
	AARR	temp, &12, a
	INC	-, -, index
	JMP	-, -, ..@l17
..@l18:
	PUSH	-, -, 0
	SUB	num, 1, &13
	PUSH	-, -, &13
	CALL	qsort, -, -
	SUB	num, 1, &14
	ASS	0, -, index
..@l19:
	GTT	index, &14, ..@l20
	LOAD	a, index, &15
	WRI	-, -, J_&15
	INC	-, -, index
	JMP	-, -, ..@l19
..@l20:
	FIN	-, -, -

